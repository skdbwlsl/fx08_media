      <Label fx:id="labelTime" layoutX="551.0" layoutY="239.0" prefHeight="26.0" prefWidth="57.0" text="0/0 sec">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="480.0" layoutY="271.0" prefHeight="26.0" prefWidth="58.0" text="소리">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Slider fx:id="sliderVolume" layoutX="481.0" layoutY="299.0" onMouseDragged="#volumeControl" prefHeight="14.0" prefWidth="200.0" showTickMarks="true" />
   </children>
</AnchorPane>
