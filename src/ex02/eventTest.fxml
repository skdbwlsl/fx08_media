<AnchorPane prefHeight="347.0" prefWidth="725.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ex02.Controller">
   <children>
      <MediaView fx:id="fxMediaView" fitHeight="250.0" fitWidth="400.0" layoutX="41.0" layoutY="49.0" />
      <Button fx:id="btnPlay" layoutX="496.0" layoutY="39.0" mnemonicParsing="false" onAction="#myPlay" prefHeight="44.0" prefWidth="132.0" text="재생">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
      <Button fx:id="btnPause" layoutX="496.0" layoutY="83.0" mnemonicParsing="false" onAction="#myPause" prefHeight="42.0" prefWidth="132.0" text="일시중지">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnStop" layoutX="496.0" layoutY="125.0" mnemonicParsing="false" onAction="#myStop" prefHeight="51.0" prefWidth="132.0" text="종료">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label layoutX="480.0" layoutY="195.0" prefHeight="26.0" prefWidth="58.0" text="시간">
